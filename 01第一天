<script type="text/javascript">
    function but_tj() {
        var userName = $("#userName").val();
        var userTel = $("#userTel").val();
        var userMail = $("#userMail").val();
        var userCompany = $("#userCompany").val();
        var userFeedback = $("#userFeedback").val();
        if (feedbackCheck()) {
            $.get("/api/cn/feed/AddFeedback?name=" + userName + "&tel=" + userTel + "&emal=" + userMail + "&comp=" + userCompany + "&count=" + userFeedback + "", function (data) {
                if (data.errcode === 1) {
                    alertUrl("留言成功！", location.href);
                } else {
                    newAlert("参数错误!");
                }
            });
        }
    }
</script>


