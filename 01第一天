<script type="text/javascript">
    function del(e) {
        var id = $(e).attr("data-id");
        var price = parseFloat($("#about_" + id).attr("data-money"));
        var sum = parseInt($("#sum").html());
        var money = parseFloat($("#money").html());
        $.get("/api/cn/cart/DeleCart?id=" + id, function (data) {
            if (data.errcode === 1) {
                $("#about_" + id).remove();
                $("#sum").html(sum - 1);
                if (sum - 1 === 0) {
                    $("#money").html(0);
                } else {
                    $("#money").html((money - price).toFixed(2));
                }

            } else {
                newAlert("参数错误!");
            }
        });
    }
    function sench(type) {
        var dd = $(".J-search-sort em").html();
        var str = $("#inputbox").val();

        if (type == 1) {
            $(".J-m-sort li").each(function myfunction() {
                if ($(this).attr("class") == "cur") {
                    dd = $(this).children().html();
                }
            });
            str = $("#searchbox").val();
        }

        switch (dd) {
            case "新闻中心":
                location.href = encodeURI("/Search/Index?id=0&type=1&keyWords=" + str + "");
                break;
            case "下载中心":
                location.href = encodeURI("/Search/Index?id=0&type=0&keyWords=" + str + "");
                break;
            case "在线购买":
                location.href = encodeURI("/Search/Index?id=0&type=2&keyWords=" + str + "");
                break;
            default:
        }
    }

    function checked() {
        $.get("/api/cn/cart/AllShoppingCart", function (data) {
            if (data.errcode === 1) {
                location.href = "/purchase/check";
            } else {
                newAlert("参数错误!");
            }
        });
    }

</script>
 
        

