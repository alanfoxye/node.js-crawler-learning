        $(function () {
            var tid = "15";
            switch (tid) {
                case "16":
                    $("#top_ul li:eq(0)").attr("class", "cur");
                    break;
                case "15":
                    $("#top_ul li:eq(0)").attr("class", "cur");
                    break;
                case "13":
                    $("#top_ul li:eq(1)").attr("class", "cur");
                    break;
                case "14":
                    $("#top_ul li:eq(2)").attr("class", "cur");
                    break;
                case "17":
                    $("#top_ul li:eq(3)").attr("class", "cur");
                    break;
                default:
            }
        });

        function AddCollect(e) {
            var id = $(e).attr("data-id");
            $.get("/api/cn/about/AddCollect?id=" + id + "&type=0", function (data) {
                if (data.errcode === 1) {
                    newAlert("收藏成功!");
                } else if (data.Flag == "Login") {
                    newAlert("请您先登录，再进行收藏！");

                } else {
                    newAlert("请您先登录，再进行收藏！");
                }
            });
        }
    </script>


